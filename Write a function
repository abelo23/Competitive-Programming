def is_leap(year):
    leap = False
    
    # Write your logic here
    if 1900 <= year <= 10**5:
        if year % 4 == 0:
            if year % 100 == 0:
                if year % 4 == 0:
                    leap = True
                else:
                    leap 
            else:
                leap = True
        else:
            leap
    else:
        None
    return leap
                
    
year = int(input())
print(is_leap(year))
